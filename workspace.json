{
  "version": 2,
  "projects": {
    "laudebugs": {
      "root": "apps/laudebugs",
      "sourceRoot": "apps/laudebugs",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/laudebugs",
            "outputPath": "dist/apps/laudebugs"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "laudebugs:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "laudebugs:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "laudebugs:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/laudebugs"],
          "options": {
            "jestConfig": "apps/laudebugs/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/laudebugs/**/*.{ts,tsx,js,jsx}"]
          },
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": []
    },
    "laudebugs-e2e": {
      "root": "apps/laudebugs-e2e",
      "sourceRoot": "apps/laudebugs-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/laudebugs-e2e/cypress.json",
            "devServerTarget": "laudebugs:serve",
            "tsConfig": "apps/laudebugs-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "laudebugs:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/laudebugs-e2e/**/*.{js,ts}"]
          },
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": [],
      "implicitDependencies": ["laudebugs"]
    }
  }
}
